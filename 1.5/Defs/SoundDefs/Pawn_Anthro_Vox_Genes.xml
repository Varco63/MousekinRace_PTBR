	<SoundDef>
		<defName>Pawn_Mousekin_Call</defName>
		<context>MapOnly</context>
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Folder">
						<clipFolderPath>Pawn/Anthro/Mousekin/Call</clipFolderPath>
					</li>
				</grains>
				<volumeRange>20</volumeRange>
				<pitchRange>0.95~1.05</pitchRange>
