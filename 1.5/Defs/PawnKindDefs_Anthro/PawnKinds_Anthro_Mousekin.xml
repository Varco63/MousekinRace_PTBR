<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<!-- Notes on initialWillRange and initialResistanceRange:
		- Mice are harder to recruit, due to their mild mistrust of humans
		- Paradoxically, they are usually easier to enslave due to their small size and weakness, and can be more quickly cowed into submission
	-->

	<PawnKindDef Abstract="True" Name="Mousekin_BasePawnKind">
		<race>Mousekin</race>
		<combatPower>20</combatPower>
		<nameMaker>NamerPerson_MousekinMale</nameMaker>
		<nameMakerFemale>NamerPerson_MousekinFemale</nameMakerFemale>
		<isFighter>false</isFighter>
		<!-- Most Mousekins (or their unseen world pawn relatives) are assumed to be from the Kingdom faction -->
		<defaultFactionType>Mousekin_FactionKingdom</defaultFactionType>
		<invFoodDef>Mousekin_MealTrailMix</invFoodDef>
		<invNutrition>3</invNutrition>
		<backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
		<apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
		<apparelMoney>350~600</apparelMoney>
		<initialWillRange>0~1</initialWillRange>
		<initialResistanceRange>16~24</initialResistanceRange>
		<xenotypeSet>
			<xenotypeChances>
				<Mousekin_XenotypeMousekin MayRequire="Ludeon.Rimworld.Biotech">1</Mousekin_XenotypeMousekin>
			</xenotypeChances>
		</xenotypeSet>
	</PawnKindDef>

	<!-- Player starting colonists -->

	<PawnKindDef ParentName="Mousekin_BasePawnKind">
		<defName>MousekinColonist</defName>
		<label>Mousekin settler</label>
		<forceNormalGearQuality>true</forceNormalGearQuality>
		<backstoryFiltersOverride>
			<li>
				<categories>
					<li>MousekinStartingColonist</li>
				</categories>
			</li>
		</backstoryFiltersOverride>
		<defaultFactionType>Mousekin_PlayerFaction_Settlers</defaultFactionType>
		<minGenerationAge>20</minGenerationAge>
		<maxGenerationAge>45</maxGenerationAge>
		<apparelTags>
			<li>MousekinStartingColonist</li>
		</apparelTags>
		<initialWillRange>0~1</initialWillRange>
		<initialResistanceRange>16~24</initialResistanceRange>
		<apparelIgnoreSeasons>true</apparelIgnoreSeasons>
	</PawnKindDef>

